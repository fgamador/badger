<%= form_for(@rank) do |f| %>
  <% form_error_messages(@rank, "rank") %>
  <% form_text_field(f, :name) %>
  <% form_text_field(f, :ordinal) %>
  <% form_text_field(f, :num_merit_badges) %>
  <% form_text_field(f, :num_eagle_required) %>
  <% form_text_field(f, :reference_url_suffix) %>
  <% form_submit_buttons(f, use_save_and_new) %>
<% end %>

